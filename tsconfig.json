{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "jsxImportSource": "@emotion/react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./components/*"
      ],
      "@/config/*": [
        "./config/*"
      ],
      "@/constant/*": [
        "./constant/*"
      ],
      "@/data/*": [
        "./data/*"
      ],
      "@/features/*": [
        "./features/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/models/*": [
        "./models/*"
      ],
      "@/providers/*": [
        "./providers/*"
      ],
      "@/store/*": [
        "./store/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "./index.d.ts",
    "cypress/tsconfig.json"
  ],
  "exclude": [
    "node_modules"
  ]
}